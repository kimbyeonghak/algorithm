#include<cstdio>

int factorial(int n){
	
	if (n == 1){
		return 1;
	}
	else{
		return n*factorial(n-1);
	}
	
}
int main(){
	int n;
	scanf("%d",&n);
	printf("%d",factorial(n));
}
///////////////
#include<cstdio>

int Position(int n){
	if (n==0){
		return 0;
	}
	else{
		return n%10 + Position(n/10);
	}
	
}
int main(){
	int n;
	scanf("%d",&n);
	printf("%d",Position(n));
}
