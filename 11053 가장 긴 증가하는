#include <cstdio>

int arr[1001];
int dp[1001];

int main() {
    int n;
    scanf("%d", &n);
    
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        dp[i] = 1;  // dp 배열 초기화
    }

    int maxLength = 1;

    for (int i = 1; i < n; i++) {
        for (int j = 0; j < i; j++) {
            if (arr[i] > arr[j] && dp[i] < dp[j] + 1) {
                dp[i] = dp[j] + 1;
            }
        }
        if (maxLength < dp[i]) {
            maxLength = dp[i];
        }
    }

    printf("%d\n", maxLength);
    return 0;
}
