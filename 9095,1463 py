t = int(input())  # 테스트 케이스 개수

dp = [0, 1, 2, 4]  # dp[1]=1, dp[2]=2, dp[3]=4 (기초값)

for i in range(4, 11):  # n은 최대 10
    dp.append(dp[i-1] + dp[i-2] + dp[i-3])

for _ in range(t):
    n = int(input())
    print(dp[n])
-------------------------------------
n = int(input())
dp = [0] * (n+1)

for i in range(2, n+1):
    dp[i] = dp[i-1] + 1
    if i % 2 == 0:
        dp[i] = min(dp[i], dp[i//2] + 1)
    if i % 3 == 0:
        dp[i] = min(dp[i], dp[i//3] + 1)

print(dp[n])
